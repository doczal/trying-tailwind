{
  "name": "trying-tailwind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.7.1",
    "formik": "^2.0.3",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "tailwindcss": "^1.1.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:styles start-react",
    "start-react": "sleep 2 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:styles": "postcss src/styles/index.css -o src/styles/tailwind.css -w",
    "build:styles": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "prebuild": "npm run build:styles",
    "prestart": "npm run build:styles"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "stylelint-config-recommended": "^3.0.0"
  }
}
